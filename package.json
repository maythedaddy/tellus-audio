{
  "name": "tellus-audio-site",
  "version": "1.0.0",
  "description": "This is the site of a fictional music software company I created. <br>",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://maythedaddy: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@github.com/maythedaddy/tellus-audio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maythedaddy/tellus-audio/issues"
  },
  "homepage": "https://github.com/maythedaddy/tellus-audio#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.62.1"
  }
}
